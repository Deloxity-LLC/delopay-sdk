{
    "name":  "@delopay/sdk",
    "version":  "0.1.0",
    "description":  "Official DeloPay TypeScript SDK",
    "type":  "module",
    "main":  "dist/index.cjs",
    "module":  "dist/index.js",
    "types":  "dist/index.d.ts",
    "files":  [
                  "dist",
                  "generated"
              ],
    "exports":  {
                    ".":  {
                              "types":  "./dist/index.d.ts",
                              "import":  "./dist/index.js",
                              "require":  "./dist/index.cjs"
                          }
                },
    "scripts":  {
                    "build":  "tsup",
                    "test":  "vitest run",
                    "lint":  "eslint \"src/**/*.ts\"",
                    "typecheck":  "tsc --noEmit"
                },
    "devDependencies":  {
                            "@types/node":  "^24.3.1",
                            "@typescript-eslint/eslint-plugin":  "^8.43.0",
                            "@typescript-eslint/parser":  "^8.43.0",
                            "eslint":  "^8.57.1",
                            "tsup":  "^8.5.0",
                            "typescript":  "^5.9.2",
                            "vitest":  "^3.2.4"
                        }
}
